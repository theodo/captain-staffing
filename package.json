{
  "name": "captain-staffing",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/theodo/captain-staffing.git"
  },
  "homepage": "https://theodo.github.io/captain-staffing",
  "license": "MIT",
  "author": "Nicolas Girault",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.59.0",
    "flow-coverage-report": "^0.4.0",
    "flow-typed": "^2.2.3",
    "gh-pages": "^1.1.0",
    "istanbul": "^0.4.5",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta",
    "immutable": "^3.8.2",
    "lodash": "^4.15.0",
    "moment": "^2.17.1",
    "nsp": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "^1.0.17",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "source-map-explorer": "^1.5.0",
    "styled-components": "^2.2.4",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "env:dev": "cp src/configs/config.dev.js src/configs/config.js",
    "env:prod": "cp src/configs/config.prod.js src/configs/config.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint:staged": "lint-staged",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report --threshold=75 -i 'src/**/*.js' -t html -t json -t text",
    "lint": "eslint --ext .jsx,.js -c .eslintrc src",
    "nsp": "nsp check",
    "start": "npm run env:dev && react-scripts start",
    "test:coverage": "npm run test -- --coverage",
    "test": "react-scripts test --env=jsdom"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": "lint"
  }
}
